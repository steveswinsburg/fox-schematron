  <xs:attribute name="map-set-attach" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>Sets the context :{map-set-attach} to the XPath specified by this attribute.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="flow">
    <xs:simpleType>
      <xs:restriction base="entered-string">
        <xs:enumeration value="across"/>
        <xs:enumeration value="down"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="debug">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="true"/>
        <xs:enumeration value="false"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="prompt2"/>
  <xs:attribute name="selector" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>XPath targeting one nested node which specifies cardinality generator for multi-select map-set widgets.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="prompt-short">
    <xs:annotation>
      <xs:documentation>A constant prompt, contains no xpaths</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="radio-owner" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>XPATH expression that locates an element that when common with another radio item, and the same radio-group label is in use, the item joins the logical radio set. The default is "." indicating the current record. This allows radio groups to be defined across records (i.e. within a common parent or super-parent list e.g. ".."  See also radio-group.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="radio-group" type="entered-string">
    <xs:annotation>
      <xs:documentation>Label string that associates distinct items to form a logical radio group. This default value is the absolute path of the item in the schema definition. This attribute allows radio groups to be defined across items. When used with radio-owner, across records can also be acheived. See also radio-owner.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="forceVisible" type="xs:boolean">
    <xs:annotation>
      <xs:documentation>Allows the columns headings to enabled/disabled if there is no data in the set-out</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="mand" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>Item is mandatory default is not mandatory.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="init-db-interface">
    <xs:annotation>
      <xs:documentation>The name of a FOX module DB Interface used to initialise a node.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="init-query">
    <xs:annotation>
      <xs:documentation>The name of a FOX module DB Interface Query used to initialise a node.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="init-xpath">
    <xs:annotation>
      <xs:documentation>An XPath expression that will be used to initialise a node.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="confirm" type="xs:string">
    <xs:annotation>
      <xs:documentation>Message displayed in a Y/N dialog box</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="inputSpellWidget">
    <xs:annotation>
      <xs:documentation>Use this attribute to turn on the abc icon next to xs:string fields for spell check.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="spell">
    <xs:annotation>
      <xs:documentation>XPath to turn on/off whether a field is subject to spell check.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="actionSpell">
    <xs:annotation>
      <xs:documentation>Similar to a run should be placed on an action or phantom to indicate that the action requires spell check to run before post.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="actionSpellOk" type="xs:string">
    <xs:annotation>
      <xs:documentation>Message to be displayed after a successful spell check.  For use on phantoms or actions.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="defaultAction">
    <xs:annotation>
      <xs:documentation>Indicates the action is the default for the form and should be run if the Enter key is pressed.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="imported">
    <xs:annotation>
      <xs:documentation>This element must be imported from the top modules schema (not a library module schema). The module will not load if an element in the current module (or one of its libraries) is duplicated in the schema without an import attribute. Also if an element has an imported attr and it does not exist in the schema the load will fail. If a element is duplicated in schema, module, and library, schema is always dominate, then first overrides left to right.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value=""/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="key-true" type="xs:string">
    <xs:annotation>
      <xs:documentation>The display key for true in boolean selector widgets. Default to Yes</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-false" type="xs:string">
    <xs:annotation>
      <xs:documentation>The display key for false in boolean selector widgets. Defaults to No.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-null" type="xs:string">
    <xs:annotation>
      <xs:documentation>The display key for null content selector widgets. Defaults to None. This is only used for optional elements. Mand elements use key-missing.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-missing" type="xs:string">
    <xs:annotation>
      <xs:documentation>The display key for null content selector widgets. Defaults to Select One|Many. This is only used for mand elements. Optinal elements use key-null.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-unrecognised" type="xs:string">
    <xs:annotation>
      <xs:documentation>The display key for unrecognised content selector widgets. Defaults to content string (if simple) otherwise Unrecognised.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-exclusive" type="xs:string">
    <xs:annotation>
      <xs:documentation>Used to nominate a mulit-select widgets key as being mutually exclusive from the other possible selections.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="file-storage-location">
    <xs:annotation>
      <xs:documentation>A storage location for any contant that can be stored in a file.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:annotation>
        <xs:documentation>The storage location where the file resides.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string"/>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="formTableClass"/>
  <xs:attribute name="formTableStyle" type="style-type"/>
  <xs:attribute name="menuTableClass"/>
  <xs:attribute name="menuTableStyle" type="style-type"/>
  <xs:attribute name="listTableClass"/>
  <xs:attribute name="listTableStyle" type="style-type"/>
  <xs:attribute name="nestedTableClass"/>
  <xs:attribute name="nestedTableStyle" type="style-type"/>
