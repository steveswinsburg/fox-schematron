    <xs:annotation>
      <xs:documentation>used with url widget</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="superPrompt"/>
  <xs:attribute name="clean">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="trim"/>
        <xs:enumeration value="no-trim"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="case">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="upper"/>
        <xs:enumeration value="lower"/>
        <xs:enumeration value="initCap"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="formatDate"/>
  <xs:attribute name="col">
    <xs:annotation>
      <xs:documentation>database column name</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="externalUrl"/>
  <xs:attribute name="ns-alias" type="xs:string">
    <xs:annotation>
      <xs:documentation>Duplicates fox attributes from one namespace to another on self and decendents. This is useful when included  type definitions with fox markup attributes are used locally. Namespaces in the included type can be aliased into local namespaces for setout operations.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="change-action">
    <xs:annotation>
      <xs:documentation>Fires an action when a piece of data changes</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="compare-id"/>
  <xs:attribute name="upload-mode">
    <xs:annotation>
      <xs:documentation>The upload mode for a file upload. Interruption refers to whether the base page sends a signal to the upload window to terminate the upload in the event of a module push or pop. Interruption can only be disabled when the upload target is a DOM in a storage location (i.e. root).</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="interrupt-on-module-push-pop-upload-target-any"/>
        <xs:enumeration value="no-interrupt-upload-target-storage-location"/>
        <xs:enumeration value="no-interrupt-modal-upload-window"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="upload-metadata">
    <xs:annotation>
      <xs:documentation>Choose the type of metadata inserted into the DOM on completion of an upload. 'Basic' stores metadata about the last successful upload. 'Diagnostic' additionally provides information about the latest upload attempt, regardless of its status. Default is basic.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="basic"/>
        <xs:enumeration value="diagnostic"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
