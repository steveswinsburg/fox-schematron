        <xs:enumeration value="west"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="promptAlignX">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="left"/>
        <xs:enumeration value="center"/>
        <xs:enumeration value="right"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="promptAlignY">
    <xs:simpleType>
      <xs:restriction base="xs:string">
